<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="googleapiPane" prefHeight="600.0" prefWidth="800.0" styleClass="root" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.java_dictionary.GoogleAPIController">
   <children>
      <ScrollPane layoutX="197.0" layoutY="6.0" prefHeight="254.0" prefWidth="491.0" styleClass="Meaning_Word">
         <content>
            <TextField fx:id="TranslatedText" editable="false" prefHeight="31.0" prefWidth="489.0" />
         </content>
      </ScrollPane>
      <ScrollPane layoutX="194.0" layoutY="341.0" prefHeight="254.0" prefWidth="491.0" styleClass="English_Word">
         <content>
            <TextField fx:id="SourceText" prefHeight="31.0" prefWidth="489.0" />
         </content>
      </ScrollPane>
      <Button fx:id="TranslateButton" layoutX="329.0" layoutY="270.0" mnemonicParsing="false" onMouseClicked="#OnTranslateButtonClicked" styleClass="Add_Button" text="Dịch" />
      <Label layoutX="18.0" layoutY="92.0" prefHeight="46.0" prefWidth="172.0" styleClass="Meaning_Title" text="Văn bản gốc" />
      <Label layoutX="40.0" layoutY="420.0" styleClass="English_Title" text="Bản dịch" />
      <Button fx:id="ChangeTargetLangButton" layoutX="461.0" layoutY="270.0" mnemonicParsing="false" onMouseClicked="#OnChangeTargetLang" styleClass="Add_Button1" text="Đổi" />
      <Label fx:id="TargetLangText" layoutX="570.0" layoutY="277.0" prefHeight="46.0" prefWidth="172.0" text="Anh sang Việt" textFill="#ececef">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
   </children>
   <stylesheets>
      <URL value="@googleapi.css" />
   </stylesheets>
</AnchorPane>
